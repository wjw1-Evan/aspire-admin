/* 
  Using SVG Mask for reliable vector icons in Mini Programs.
  This approach avoids font-loading issues and works on both iOS/Android.
*/

.iconfont {
  display: inline-block;
  width: 1em;
  height: 1em;
  background-color: currentColor;
  -webkit-mask-size: contain;
  mask-size: contain;
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  vertical-align: middle;
}

